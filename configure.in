dnl
dnl Process this file with autoconf to produce a configure script.
dnl
AC_INIT()

AC_PROG_CC
AC_PROG_INSTALL

AC_CONFIG_SUBDIRS(parser)

dnl Build release name
changequote(<,>)
REL=`echo 'substr($Name,0)$' | tr -d '$: \-' | sed 's/^[A-Za-z]*//' | sed 's/\_/\./'`
VER="$REL `date '+%m/%d/%Y'`"
changequote([,])
AC_DEFINE_UNQUOTED(VERSION, "$VER")

AC_OUTPUT(Makefile)
